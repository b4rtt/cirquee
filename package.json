{
  "name": "cirquee",
  "version": "1.0.0",
  "license": "MIT",
  "main": "expo-router/entry",
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock ios android && yarn install",
    "prebuild": "yarn expo prebuild",
    "start": "NODE_ENV=production yarn expo start -c",
    "start:dev": "NODE_ENV=development yarn expo start -c",
    "android": "DARK_MODE=media NODE_ENV=production yarn expo start --android",
    "android:dev": "NODE_ENV=development yarn expo start --android",
    "ios": "DARK_MODE=media NODE_ENV=production yarn expo start --ios",
    "ios:dev": "NODE_ENV=development yarn expo start --ios",
    "format": "yarn prettier --config .prettierrc src/**/*.{ts,tsx} --write",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "web": "DARK_MODE=media undefined"
  },
  "dependencies": {
    "@expo/html-elements": "0.4.2",
    "@gluestack-ui/alert": "^0.1.16",
    "@gluestack-ui/alert-dialog": "^0.1.32",
    "@gluestack-ui/button": "^1.0.8",
    "@gluestack-ui/icon": "^0.1.25",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.16",
    "@gluestack-ui/toast": "^1.0.8",
    "@legendapp/motion": "^2.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "~52.0.25",
    "expo-constants": "~17.0.4",
    "expo-font": "~13.0.1",
    "expo-image": "~2.0.2",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~7.0.4",
    "expo-router": "~4.0.16",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.7",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-css-interop": "^0.1.22",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "tailwindcss": "^3.4.17",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jscodeshift": "0.15.2",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "private": true
}
