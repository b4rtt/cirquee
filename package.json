{
  "name": "cirquee",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock && yarn install",
    "start": "NODE_ENV=production yarn expo start -c",
    "start:dev": "NODE_ENV=development yarn expo start -c",
    "android": "NODE_ENV=production yarn expo start --android",
    "android:dev": "NODE_ENV=development yarn expo start --android",
    "ios": "NODE_ENV=production yarn expo start --ios",
    "ios:dev": "NODE_ENV=development yarn expo start --ios",
    "doctor:dev": "EXPO_NO_DOTENV=1 dotenv -e .env.development -- yarn expo-doctor",
    "format": "prettier --config .prettierrc src/**/*.{ts,tsx} --write",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "expo": "~52.0.25",
    "expo-font": "~13.0.1",
    "expo-image": "~2.0.2",
    "expo-linear-gradient": "~14.0.1",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "private": true
}
